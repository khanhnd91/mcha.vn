/************************************************

	Area Select　（0307_言語選択追加による新規追加）

************************************************/
.droppy {
	display: block;
	margin: 0;
	padding: 0;
	position: relative;
}
.droppy li {
	display: block;
	list-style: none;
	margin: 0;
	padding: 0;
	float: left;
	position: relative;
}
.droppy a {
	display: block;
	color: #fff;
	text-decoration: none;
}
.droppy a:visited {
	color: #fff;
}
.droppy ul {
	display: none;
	position: absolute;
	left: 0;
	margin: 0;
	padding: 0;
}
* html .droppy ul {
	line-height: 0;
} /* IE6 "fix" */
.droppy ul a {
	zoom: 1;
} /* IE6/7 fix */
.droppy ul li {
	float: none;
}
.droppy ul ul {
	top: 0;
}
/* Essentials - configure this */
.droppy ul {
	width: 200px;
}
.droppy ul ul {
	left: 131px;
}
/* Everything else is theming */
.droppy {
	background-color: #424242;
	float: left;
}
.droppy *:hover {
	background-color: none;
}
.droppy a {
	border-right: 1px solid white;
	color: #fff;
	font-size: 12px;
	padding: 14px;
	line-height: 1;
	font-weight: 400;
}
.droppy li.hover a {
	background-color: #5E5A5A;
	color: #fff;
}
.droppy ul li a {
	background-color: #5E5A5A;
}
.droppy ul a.has-subnav {
	background-color: #2E2A2A;
}
.droppy ul a.hover {
	background-color: #ABB118;
}
.droppy ul a {
	border-bottom: 1px solid white;
	border-right: none;
	opacity: 0.9;
	filter: alpha(opacity=90);
}
/* .droppy ul a { border-bottom: none; } - I also needed this for IE6/7 */


